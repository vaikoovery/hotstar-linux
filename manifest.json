{
    "manifest_version": 2,
    "name": "hotstar",
    "version": "0.2",
    "browser_action": {
    "default_icon": "icon.png"
    },
    "content_scripts": [
    {
        "matches": [
        "*://*.hotstar.com/*"
        ],
        "js": ["jquery-3.1.1.min.js","content.js"]
    }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs"
    ]
}